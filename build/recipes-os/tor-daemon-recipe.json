{
  "name": "tor-daemon",
  "version": "0.4.7.10",
  "description": "The Tor anonymity network daemon.",
  "source": {
    "type": "http",
    "url": "https://dist.torproject.org/tor-0.4.7.10.tar.gz",
    "sha256": "YOUR_SHA256_HERE_FOR_TOR_TARBALL"
  },
  "dependencies": [
    "openssl",
    "libevent",
    "zlib"
  ],
  "build": {
    "commands": [
      "./configure --prefix=/usr --disable-asciidoc",
      "make -j$(nproc)"
    ]
  },
  "install": {
    "commands": [
      "make DESTDIR=$DESTDIR install"
    ]
  },
  "provides": ["/bin/tor"]
}