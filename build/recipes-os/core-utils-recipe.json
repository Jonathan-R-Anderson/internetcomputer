{
  "name": "core-utils",
  "version": "1.0.0",
  "description": "Core utilities for WorldComputer (ls, cat, echo, etc.).",
  "source": {
    "type": "git",
    "url": "https://your-git-server.com/worldcomputer/core-utils.git",
    "revision": "v1.0.0"
  },
  "dependencies": [
    "libc"
  ],
  "build": {
    "commands": [
      "make all"
    ]
  },
  "install": {
    "commands": [
      "make DESTDIR=$DESTDIR install"
    ]
  },
  "provides": [
    "/bin/ls", "/bin/cat", "/bin/echo"
  ]
}